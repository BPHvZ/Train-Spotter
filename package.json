{
  "name": "train-spotter",
  "version": "0.0.0",
  "scripts": {
    "---------------- Serve Application ----------------": "ng",
    "serve": "ng serve --hmr",
    "serve_no_hmr": "ng serve",
    "pre_serve_production": "ng build --prod --outputHashing=all",
    "_serve_production": "yarn pre_serve_production && http-server -p 8080 -c-1 dist/trainSpotter",
    "---------------- Inspect Bundles ----------------": "",
    "prebundle-report": "ng build --prod --stats-json",
    "bundle-report": "yarn prebundle-report && webpack-bundle-analyzer dist/trainSpotter/stats.json",
    "---------------- Build Application ----------------": "ng",
    "build": "ng build",
    "prebuild:release": "yarn install --immutable",
    "build:release": "yarn prebuild:release && yarn test:release && ng build --prod --outputHashing=all",
    "---------------- Test Application ----------------": "ng",
    "test:release": "ng t --configuration=ci-release",
    "test": "ng test",
    "e2e": "ng e2e",
    "---------------- Lint Application ----------------": "ng",
    "prettier": "prettier --write .",
    "lint": "ng lint --fix",
    "format:fix": "pretty-quick --staged",
    "prettier_and_lint": "yarn format:fix && yarn lint",
    "---------------- Document Application ----------------": "ng",
    "compodoc": "compodoc --config .compodocrc.json",
    "documentation_coverage": "compodoc --config .compodocrc.json --coverageTest 95 --coverageTestThresholdFail=true",
    "---------------- Other ----------------": "ng",
    "update_sitemap": "yarn node src/environments/update-sitemap.js"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 9-10",
    "not IE 11"
  ],
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "dependencies": {
    "@angular/animations": "^11.2.12",
    "@angular/cdk": "11.2.11",
    "@angular/common": "^11.2.12",
    "@angular/compiler": "^11.2.12",
    "@angular/core": "^11.2.12",
    "@angular/forms": "^11.2.12",
    "@angular/localize": "^11.2.12",
    "@angular/material": "11.2.11",
    "@angular/platform-browser": "^11.2.12",
    "@angular/platform-browser-dynamic": "^11.2.12",
    "@angular/router": "^11.2.12",
    "@angular/service-worker": "^11.2.12",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@types/node": "^14.14.43",
    "bootstrap": "^4.6.0",
    "easytimer.js": "^4.3.4",
    "jimp": "^0.14.0",
    "mapbox-gl": "^2.2.0",
    "ng-inline-svg": "^12.1.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-mapbox-gl": "^6.0.4",
    "replace-color": "^2.3.0",
    "rxjs": "^6.6.7",
    "rxjs-pausable": "^1.0.0",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.11",
    "@angular-eslint/builder": "4.2.0",
    "@angular-eslint/eslint-plugin": "4.2.0",
    "@angular-eslint/eslint-plugin-template": "4.2.0",
    "@angular-eslint/schematics": "4.2.0",
    "@angular-eslint/template-parser": "4.2.0",
    "@angular/cli": "^11.2.11",
    "@angular/compiler-cli": "^11.2.12",
    "@compodoc/compodoc": "^1.1.11",
    "@types/http-server": "^0.10.1",
    "@types/jasmine": "^3.6.10",
    "@types/mapbox-gl": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.1",
    "depcheck": "^1.4.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jsdoc": "^33.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-rxjs": "^3.3.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
