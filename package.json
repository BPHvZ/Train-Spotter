{
  "name": "train-spotter",
  "version": "0.0.0",
  "scripts": {
    "---------------- Serve Application ----------------": "ng",
    "serve": "ng serve --hmr",
    "serve_no_hmr": "ng serve",
    "_serve_production": "http-server -p 8080 -c-1 dist/trainSpotter",
    "pre_serve_production": "ng build --prod --outputHashing=all",
    "---------------- Inspect Bundles ----------------": "",
    "prebundle-report": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/trainSpotter/stats.json",
    "---------------- Build Application ----------------": "ng",
    "build": "ng build",
    "build:release": "yarn run test:release && ng build --prod --outputHashing=all",
    "prebuild:release": "yarn install --frozen-lockfile --ignore-optional",
    "---------------- Test Application ----------------": "ng",
    "test:release": "ng t --configuration=ci-release",
    "test": "ng test",
    "e2e": "ng e2e",
    "---------------- Lint Application ----------------": "ng",
    "prettier": "prettier --write .",
    "lint": "ng lint --fix",
    "format:fix": "pretty-quick --staged",
    "prettier_and_lint": "run-s format:fix lint",
    "---------------- Document Application ----------------": "ng",
    "compodoc": "npx compodoc --config .compodocrc.json",
    "documentation_coverage": "npx compodoc --config .compodocrc.json --coverageTest 80 --coverageTestThresholdFail=true",
    "---------------- Other ----------------": "ng",
    "update_sitemap": "node src/environments/update-sitemap.js"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 9-10",
    "not IE 11"
  ],
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@angular/animations": "^11.2.5",
    "@angular/cdk": "11.2.5",
    "@angular/common": "^11.2.5",
    "@angular/compiler": "^11.2.5",
    "@angular/core": "^11.2.5",
    "@angular/forms": "^11.2.5",
    "@angular/localize": "^11.2.5",
    "@angular/material": "11.2.5",
    "@angular/platform-browser": "^11.2.5",
    "@angular/platform-browser-dynamic": "^11.2.5",
    "@angular/router": "^11.2.5",
    "@angular/service-worker": "^11.2.5",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@types/node": "^14.14.31",
    "bootstrap": "^4.5.3",
    "easytimer.js": "^4.3.4",
    "jimp": "^0.14.0",
    "mapbox-gl": "^2.1.1",
    "ngx-mapbox-gl": "^6.0.4",
    "replace-color": "^2.3.0",
    "rxjs": "^6.6.3",
    "rxjs-pausable": "^1.0.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.4",
    "@angular-eslint/builder": "^2.0.0",
    "@angular-eslint/eslint-plugin": "^2.0.0",
    "@angular-eslint/eslint-plugin-template": "^2.0.0",
    "@angular-eslint/schematics": "^2.0.1",
    "@angular-eslint/template-parser": "^2.0.0",
    "@angular/cli": "^11.2.4",
    "@angular/compiler-cli": "^11.2.5",
    "@types/jasmine": "^3.6.4",
    "@types/mapbox-gl": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "codelyzer": "^6.0.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-teamcity-reporter": "^1.1.0",
    "lint-staged": "^10.5.4",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "pretty-quick": "^3.1.0",
    "protractor": "^7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.4",
    "w3c-hr-time": "^1.0.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "xml-formatter": "^2.4.0",
    "xml2json": "^0.12.0"
  },
  "resolutions": {
    "jimp": "^0.14.0",
    "replace-color/jimp": "^0.14.0"
  }
}
