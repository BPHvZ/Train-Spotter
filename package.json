{
  "name": "train-spotter",
  "version": "0.0.0",
  "scripts": {
    "---------------- Serve Application ----------------": "ng",
    "serve": "ng serve --hmr",
    "serve_no_hmr": "ng serve",
    "pre_serve_production": "ng build --outputHashing=all",
    "_serve_production": "yarn pre_serve_production && http-server -p 8080 -c-1 dist/trainSpotter",
    "---------------- Inspect Bundles ----------------": "",
    "prebundle-report": "ng build --stats-json",
    "bundle-report": "yarn prebundle-report && webpack-bundle-analyzer dist/trainSpotter/stats.json",
    "prebuild-sme": "ng build --source-map --stats-json",
    "sme-report": "yarn prebuild-sme && source-map-explorer dist/trainSpotter/**/*.js",
    "---------------- Build Application ----------------": "ng",
    "prebuild:release": "yarn install --immutable",
    "build:release": "yarn prebuild:release && yarn test:release && ng build --outputHashing=all",
    "---------------- Test Application ----------------": "ng",
    "test:release": "ng test --karma-config=karma-ci.conf.js --no-progress",
    "test": "ng test",
    "e2e": "ng e2e",
    "---------------- Lint Application ----------------": "ng",
    "prettier": "prettier --write .",
    "lint": "ng lint --fix",
    "format:fix": "pretty-quick --staged",
    "prettier_and_lint": "yarn format:fix && yarn lint",
    "---------------- Document Application ----------------": "ng",
    "compodoc": "compodoc --config .compodocrc.json",
    "documentation_coverage": "compodoc --config .compodocrc.json --coverageTest 95 --coverageTestThresholdFail=true",
    "---------------- Other ----------------": "ng",
    "update_sitemap": "yarn node src/environments/update-sitemap.js"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 9-10",
    "not IE 11"
  ],
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/forms": "^13.1.1",
    "@angular/localize": "^13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@angular/router": "^13.1.1",
    "@angular/service-worker": "^13.1.1",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@turf/line-split": "^6.5.0",
