{
  "name": "train-spotter",
  "version": "0.0.0",
  "scripts": {
    "---------------- Serve Application ----------------": "ng",
    "serve": "ng serve --hmr",
    "serve_no_hmr": "ng serve",
    "pre_serve_production": "ng build --outputHashing=all",
    "_serve_production": "yarn pre_serve_production && http-server -p 8080 --cors -S=true -C='/Users/bart/server.crt' -K='/Users/bart/server.key' -c-1 dist/trainSpotter",
    "---------------- Inspect Bundles ----------------": "",
    "prebundle-report": "ng build --stats-json",
    "bundle-report": "yarn prebundle-report && webpack-bundle-analyzer dist/trainSpotter/stats.json",
    "prebuild-sme": "ng build --source-map --stats-json",
    "sme-report": "yarn prebuild-sme && source-map-explorer dist/trainSpotter/**/*.js",
    "---------------- Build Application ----------------": "ng",
    "build:release": "ng build --outputHashing=all",
    "---------------- Test Application ----------------": "ng",
    "test:release": "ng test --karma-config=karma-ci.conf.js --no-progress",
    "test": "ng test",
    "e2e": "ng e2e",
    "---------------- Lint Application ----------------": "ng",
    "prettier": "prettier --write .",
    "lint": "ng lint --fix",
    "format:fix": "pretty-quick --staged",
    "prettier_and_lint": "yarn format:fix && yarn lint",
    "---------------- Document Application ----------------": "ng",
    "compodoc": "compodoc --config .compodocrc.json",
    "documentation_coverage": "compodoc --config .compodocrc.json --coverageTest 95 --coverageTestThresholdFail=true",
    "---------------- Other ----------------": "ng",
    "update_sitemap": "yarn node src/environments/update-sitemap.js"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 9-10",
    "not IE 11"
  ],
